{
    "name": "iris",
    "description": "a discord bot i wrote",
    "packageManager": "yarn@4.2.2",
    "type": "module",
    "dependencies": {
        "@prisma/client": "5.13.0",
        "chalk": "^5.3.0",
        "discord.js": "^14.15.2"
    },
    "scripts": {
        "replace-paths": "node scripts/module-paths.mjs",
        "rm": "rm -r dist",
        "build": "yarm rm && yarn tsc && yarn replace-paths",
        "watch": "yarn tsc --watch",
        "dev": "yarn build && node dist/init.js"
    },
    "devDependencies": {
        "@eslint/js": "^9.2.0",
        "eslint": "^9.2.0",
        "prettier": "3.2.5",
        "prisma": "^5.13.0",
        "typescript": "^5.4.5",
        "typescript-eslint": "^7.8.0"
    }
}
